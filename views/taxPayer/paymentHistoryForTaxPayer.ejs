        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Payment History</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <!-- <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Dashboard v1</li> -->
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

 
        <!-- Main content -->
        <section class="content">
          <div class="container-fluid">
            <div class="row">
              <div class="col-12">
               <div class="d-flex justify-content-end">
                <!-- <button type="button" class="btn text-primary  mb-3 " data-toggle="modal" data-target="#addModal" style="background:transparent; border: 1px solid blue;">
                  Register              
                  <i class="m-1 fa-solid fa-address-card"></i>
                 </button> -->
               </div>
                <div class="card">
                  <!-- <div class=" text-center h3 p-3">LIST OF TAX PAYERS</div> -->
                  <div class="card-body">
                    <table id="userManagementTable" class="table  ">
                        <thead>
      
                          
                          <tr>
                            <th>Payment ID</th>
                            <th>Full name</th>              
                            <th>Tax amout paid</th>
                            <th>Cash tendered</th>
                            <th>Change</th>
                            <th>Payment date</th>
                            <th>Payment Status</th>
                          </tr>
                        </thead>
                        
                        <tbody>
                            <% if(locals.paymentHistory) { %>
                              <% paymentHistory.forEach(function(payments) { %>
                            <tr>
                              <td><%= payments.id %></td>
                              <td><%= payments.firstname%> <%= payments.lastname%></td>
                              <td class="text-success"><%= payments.total_tax_amount%></td>
                              <td class="text-info"><%= payments.cash_tendered%></td>         
                              <td><%= payments.change%></td>
                              <td><%= new Date(payments.payment_date).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) %></td>               
                              <td><p class="badge badge-success"><%= payments.status %></p></td>
                          </tr>
                          <% }) %>
                          <% } %>                       
                        </tbody>
                      </table>
                  </div>
                  <!-- /.card-body -->
                </div>
                <!-- /.card -->
              </div>
              <!-- /.col -->
            </div>
            <!-- /.row -->
          </div>
          <!-- /.container-fluid -->
