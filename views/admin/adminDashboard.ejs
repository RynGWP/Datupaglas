            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

 
    
    <!-- Main content -->
    <section class="content" id="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
       
          <!-- ./col -->
          <div class="col-lg-4 col-6">
            <!-- small box -->
            <div class="small-box " style="background-color: #031a5eb4;
            box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);
          backdrop-filter: blur(4px) saturate(180%);
          -webkit-backdrop-filter: blur(2px) saturate(180%);
            "> 
              <div class="inner">
                <p style="color:aliceblue">Tax Payers</p>
                <% if (locals.taxpayersCount) { %>
                <h3 style="color:#00ff00; text-align: center;"><%= taxpayersCount %></h3>
                  <% }  else { %>
                    <h3 style="color:#00ff00">0</h3>
                    <% } %>
              </div>
            
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-4 col-6">
            <!-- small box -->
            <div class="small-box " style="background-color: #031a5eb4;
            box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);
          backdrop-filter: blur(4px) saturate(180%);
          -webkit-backdrop-filter: blur(2px) saturate(180%);
            ">
              <div class="inner">
                <% if (locals.assessorCount > 1) { %>
                  <p style="color:aliceblue">Assessors</p>
                  <h3 style="color:#00ff00 ; text-align: center;"><%= assessorCount %></h3>
                <% } else if (locals.assessorCount == 1 ) { %>
                  <p style="color:aliceblue">Assessor</p>
                  <h3 style="color:#00ff00 ; text-align: center;"><%= assessorCount %></h3>
                  
                <% } else { %>
                  <h3 style="color:#00ff00 ; text-align: center;">0</h3>
                  <p style="color:aliceblue">Assessor</p>
                  <% } %>
              </div>
            
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-4 col-6">
            <!-- small box -->
            <div class="small-box " style="background-color: #031a5eb4;
            box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);
          backdrop-filter: blur(4px) saturate(180%);
          -webkit-backdrop-filter: blur(2px) saturate(180%);
            ">
              <div class="inner">
                <% if (locals.treasurerCount > 1) { %>
                  <p style="color:aliceblue">Treasurers</p>
                  <h3 style="color:#00ff00; text-align: center;"><%= treasurerCount %></h3>
                <% } else if (locals.treasurerCount == 1) { %>
                  <p style="color:aliceblue">Treasurer</p>
                  <h3 style="color:#00ff00; text-align: center;"><%= treasurerCount %></h3>
                  <% } else { %>
                    <p style="color:aliceblue">Treasurer</p>
                    <h3 style="color:#00ff00 ; text-align: center;">0</h3>
                    <% } %>
              </div>
              
              <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
            </div>
          </div>
          <!-- ./col -->
         
         
