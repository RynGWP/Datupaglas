    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
           <div class="d-flex justify-content-end">
            <button type="button" class="btn text-primary  mb-3 " data-toggle="modal" data-target="#addModal" style="background:transparent; border: 1px solid blue;">
              Register              
              <i class="m-1 fa-solid fa-address-card"></i>
             </button>
           </div>
            <div class="card">
              <div class="card-header">
                <div class="card-title"> <h5>List of Tax payers</h5></div>
              </div>
              <div class="card-body">
                <table id="userManagementTable" class="table ">
                  <thead>

                    
                    <tr>
                      <th>Full Name</th>
                      <th>Date of Birth</th>
                      <th>Phone</th>
                      <th>Gender</th>
                      <th>Complete Address</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  
                  <tbody>
                      <% if(locals.taxPayers) { %>
                        <% taxPayers.forEach(function(taxPayer) { %>

                      <tr>
                        <td class="d-flex align-items-center">
                          <img src="<%= taxPayer.picture %>" width="40"   alt="User Image" >  
                        <span class="d-flex flex-column mx-3"> <p style="color:black; font-weight: 500; font-size: 1.3rem !important;"> <%= taxPayer.firstname %> <%= taxPayer.lastname %> <br></p>
                          <p class="text-muted fs-5"><%= taxPayer.email%></p>
                        </span>
                          
                          
                        </td>
                        <td class="align-middle"><%= new Date(taxPayer.date_of_birth).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) %></td>
                        <td class="align-middle"><%= taxPayer.phone%></td>
                        <td class="align-middle"><%= taxPayer.gender%></td>
                        <td class="align-middle"><%= taxPayer.complete_address%></td>         
                        <!-- Action -->
                        <td class="align-middle">
                          <div class="d-none d-flex justify-content-center" style="gap:3px">
                
                            <!-- View Button -->
                            <form action="/taxPayerProfile" method="post">
                              <input type="hidden" name="taxpayer_id"  value="<%= taxPayer.taxpayer_id %>">
                              <button type="submit" class="btn btn-primary btn-sm text-primary" aria-label="Edit Patient" style="background: transparent; border: none;">
                                <svg xmlns="http://www.w3.org/2000/svg" height="20" width="22.5" viewBox="0 0 576 512"><!--!Font Awesome Free 6.7.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.--><path fill="#74C0FC" d="M0 96l576 0c0-35.3-28.7-64-64-64L64 32C28.7 32 0 60.7 0 96zm0 32L0 416c0 35.3 28.7 64 64 64l448 0c35.3 0 64-28.7 64-64l0-288L0 128zM64 405.3c0-29.5 23.9-53.3 53.3-53.3l117.3 0c29.5 0 53.3 23.9 53.3 53.3c0 5.9-4.8 10.7-10.7 10.7L74.7 416c-5.9 0-10.7-4.8-10.7-10.7zM176 192a64 64 0 1 1 0 128 64 64 0 1 1 0-128zm176 16c0-8.8 7.2-16 16-16l128 0c8.8 0 16 7.2 16 16s-7.2 16-16 16l-128 0c-8.8 0-16-7.2-16-16zm0 64c0-8.8 7.2-16 16-16l128 0c8.8 0 16 7.2 16 16s-7.2 16-16 16l-128 0c-8.8 0-16-7.2-16-16zm0 64c0-8.8 7.2-16 16-16l128 0c8.8 0 16 7.2 16 16s-7.2 16-16 16l-128 0c-8.8 0-16-7.2-16-16z"/></svg>
                              </button>
                            </form>
                          
                          <!-- Delete Button -->
                          <form action="/taxpayer/delete/<%= taxPayer.taxpayer_id %>" method="POST" style="display:inline;">
                          <button type="submit" class="btn btn-danger btn-sm deleteTaxPayer text-danger fw-bold" aria-label="Delete Patient" style="background: transparent; border: none;">
                          <svg xmlns="http://www.w3.org/2000/svg" width="25" height="20" fill="#d62828" class="bi bi-trash" viewBox="0 0 16 16">
                          <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zM8 5.5a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zM11.5 6a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z"/>
                          <path d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4H2.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4zM2.5 3h11V2h-11z"/>
                          </svg>
                          </button> 
                          </form>
                      </td>
                      <!-- /Action -->
                    </tr>

                    <% }) %>
                    <% } %>
                  </tbody>
                  
                </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
