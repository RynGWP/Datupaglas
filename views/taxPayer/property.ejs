   <!-- preloader -->
   <%- include('../partials/preloader.ejs') %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Property</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <!-- <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Dashboard v1</li> -->
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

 
    
    <section class="content" id="content">
      <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col-md-10">
            <% if (locals.properties) { %>
            <header class="mb-4 text-center">
              <h1 class="fw-bold text-uppercase">Property Information</h1>
            </header>
            <table class="table table-bordered ">
              <tbody>
                <% const labels = ['Property Location', 'Property Type', 'Assessed Value', 'Area Size', 'Tax Rate', 'Ownership Type', 'Property Use', 'Classification', 'Occupancy Status', 'Last Assessment Date', 'Next Assessment Date']; %>
                <% const keys = ['property_location', 'property_type', 'assessed_value', 'area_size', 'tax_rate', 'ownership_type', 'property_use', 'classification', 'occupancy_status', 'last_assessment_date', 'next_assessment_date']; %>
                <% labels.forEach((label, index) => { %>
                  <% const key = keys[index]; %>
                  <% let displayValue = properties[key]; %>
                  <% if (label.includes('Date')) { %>
                    <% displayValue = new Date(displayValue).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }); %>
                  <% } else if (label === 'Area Size') { %>
                    <% displayValue += ' sq. m.'; %>
                  <% } else if (label === 'Tax Rate') { %>
                    <% displayValue += '%'; %>
                  <% } %>
                  <tr>
                    <th scope="row" class="fw-bold text-uppercase"><%= label %></th>
                    <td class="text-muted"><%= displayValue %></td>
                  </tr>
                <% }) %>
              </tbody>
            </table>
            <% } %>
          </div>
        </div>
      </div>
    </section>
    
   
    




  
  

  </div>
